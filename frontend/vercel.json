{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "installCommand": "npm install",
  "git": {
    "deploymentEnabled": {
      "development": true,
      "main": true,
      "master": true,
      "production": true,
      "preview": true
    }
  },
  "ignoreCommand": "echo \"Not ignored\"",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
} 