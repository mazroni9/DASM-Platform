{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "git": {
    "deploymentEnabled": {
      "development": true,
      "master": false
    }
  }
} 